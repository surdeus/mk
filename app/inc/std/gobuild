TARGNAMES = `cd src/cmd && goblin ls && cd ..`
TARG = ${TARGNAMES:%=exe/%}
STRIPTARG = ${TARG:%=%.strip}

all :V: build
strip :V: $STRIPTARG
%.strip : %
	cp -f $prereq $target
	$STRIP $target

build :V: build-$MKSHELL
build-rc :V:
	mkdir -p exe
	pwd=`{pwd}
	for(name in $TARGNAMES){
		cd src/cmd/$name
		go build -o $pwd/exe/$name
		cd $pwd
	}
build-sh :V:
	mkdir -p exe
	pwd=`pwd`
	for name in `cd src/cmd && goblin ls && cd $pwd`; do
		cd src/cmd/$name
		go build -o $pwd/exe/$name
		cd $pwd
	done
clean:V:
	rm -f exe/*
